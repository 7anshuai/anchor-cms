	"description": "Just another cms",
	"license": "GPL-3.0",
	"type": "project",
	"authors": [
		{
			"name": "Idiot",
			"email": "iam@visualidiot.com",
			"homepage": "http://visualidiot.com",
			"role": "Designer"
		},
		{
			"name": "Kieron",
			"email": "hello@madebykieron.co.uk",
			"homepage": "http://madebykieron.co.uk",
			"role": "Developer"
		}
	],
	"repositories": [
		{
            "type": "package",
            "package": {
                "name": "rwarasaurus/forms",
                "version": "dev-master",
                "dist": {
                    "url": "https://github.com/rwarasaurus/forms/archive/master.zip",
                    "type": "zip"
                }
            }
        },
		{
            "type": "package",
            "package": {
                "name": "rwarasaurus/router",
                "version": "dev-master",
                "dist": {
                    "url": "https://github.com/rwarasaurus/router/archive/master.zip",
                    "type": "zip"
                }
            }
        },
		{
            "type": "package",
            "package": {
                "name": "rwarasaurus/session",
                "version": "dev-master",
                "dist": {
                    "url": "https://github.com/rwarasaurus/session/archive/master.zip",
                    "type": "zip"
                }
            }
        },
		{
            "type": "package",
            "package": {
                "name": "rwarasaurus/validation",
                "version": "dev-master",
                "dist": {
                    "url": "https://github.com/rwarasaurus/validation/archive/master.zip",
                    "type": "zip"
                }
            }
        },
		{
            "type": "package",
            "package": {
                "name": "ircmaxell/tari-php",
                "version": "dev-master",
                "dist": {
                    "url": "https://github.com/ircmaxell/tari-php/archive/master.zip",
                    "type": "zip"
                }
            }
        }
	],
	"require": {
		"php": ">=7",
		"ircmaxell/tari-php": "dev-master",
		"rwarasaurus/forms": "dev-master",
		"rwarasaurus/validation": "dev-master",
		"rwarasaurus/session": "dev-master",
		"rwarasaurus/router": "dev-master",
		"pimple/pimple": "~3",
		"guzzlehttp/psr7": "~1",
		"doctrine/dbal": "~2",
		"mustache/mustache": "~2",
		"league/commonmark": "~0.1",
		"league/flysystem": "~1",
		"bjeavons/zxcvbn-php": "~0.1",
		"symfony/event-dispatcher": "~3",
		"symfony/console": "~3",
		"cocur/slugify": "~2"
	},
	"require-dev": {
		"phpspec/phpspec": "~2",
		"friendsofphp/php-cs-fixer": "~1"
	},
	"config": {
		"bin-dir": "bin"
	},
	"autoload": {
		"psr-4": {
			"Anchorcms\\": "app/src/"
		}
	}
}
